{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@styles/*": [
        "./styles/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}